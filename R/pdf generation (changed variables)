library("data.table")
df <- fread("heart_011.csv")
#todo INTERPOLATION/RESAMPLING here
df$min <- round(df$V1 /60)
ldf <- split(df, df$min)
pdf("visualtemp11.pdf",w=16,h=9)
par(mfrow=c(2,1))
for(d in ldf){
h <- round(d$V1[1] / 3600,3)
title <- paste0("Starts at h = ", h)
print(title)
plot(V2 ~ V1, d, type='l', main=title)

}
dev.off()
